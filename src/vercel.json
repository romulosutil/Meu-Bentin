{
  "framework": "vite",
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci --legacy-peer-deps --no-package-lock --prefer-offline",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "buildEnv": {
    "NODE_ENV": "production"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {},
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}